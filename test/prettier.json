{
  tabWidth: 4,
  semi: false,
  trailingComma: 'none',
  singleQuote: true,
  printWidth: 100,
  arrowParens: 'avoid',
  endOfLine: 'auto',
  useTabs: false,
  quoteProps: 'as-needed',
  jsxSingleQuote: false,
  jsxBracketSameLine: false,
  rangeStart: 0,
  rangeEnd: Infinity,
  requirePragma: false,
  insertPragma: false,
  proseWrap: 'preserve',
  htmlWhitespaceSensitivity: 'css'
}